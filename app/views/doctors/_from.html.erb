<%= form_with model: @doctor do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>

  <%= f.label :specialization %>
  <%= f.text_field :specialization %><br>

  <%= f.label :medical_license_number %>
  <%= f.text_field :medical_license_number %><br>

  <%= f.label :phone %>
  <%= f.text_field :phone %><br>

  <%= f.submit %>
<% end %>